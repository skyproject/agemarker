<UserControl x:Class="Agemarker.Pages.WizardPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="800">

    <Grid>
        <Grid x:Name="panelLayout" Margin="0,45,10,50"/>
        <Button x:Name="buttonNext" Content="Next" Margin="0,0,10,10" Height="35" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="80" Click="next" FontSize="14"/>
        <Button x:Name="buttonBack" Content="Back" Margin="0,0,95,10" Height="35" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="80" Click="back" FontSize="14"/>
        <Label x:Name="labelInfo" Content="Welcome to Agemarker Calcualtion Wizard! Do you want to load input data from results file?" Margin="10,10,50,0" FontSize="14" Height="30" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
